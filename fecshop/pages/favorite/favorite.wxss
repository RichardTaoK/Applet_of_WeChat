/* pages/favorite/favorite.wxss */
.account-container{
  padding: 10rpx 20rpx 40rpx;
  border-top: 2rpx solid #999; 
}
.favorite-list{
  height: 246rpx;
  display: flex;
  border-bottom: 2rpx solid #999;
  align-items: center;
}
.favorite-list .pic{
  width: 194rpx;
  height: 244rpx;
  padding: 44rpx 20rpx;
  
}
.favorite-list .my-pic{
  height: 150rpx;
  width: 150rpx;
  background: no-repeat center center/100% auto;
}
.favorite-list .nav{
  width: 500rpx;
  height: 244rpx;
  padding: 40rpx 20rpx;
  font-size: 8pt;
}
.nav .nav-list{
  margin: 4rpx 0;
}
.nav .center{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.item-price .price{
  text-decoration-line: line-through;
}
.center .delete{
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}