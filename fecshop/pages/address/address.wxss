/* pages/address/address.wxss */
.title{
  height: 74rpx;
  border-bottom: 2rpx solid #4c6b99;
  display: flex;
}
.title view{
  font-size: 10pt;
  padding: 20rpx 40rpx;
}
.title .title-name,.title-operate{
  width: 30%;
}
.title .title-address{
  width: 40%;
}
.nav{
  height: 126rpx;
  border-bottom: 2rpx solid #e0e0e0;
  display: flex;
}
.nav .nav-box{
  padding: 20rpx;
  font-size: 8pt;
  line-height: 84rpx;
}
.nav-name{
  width: 30%;
}
.nav-operate{
  width: 30%;
  display: flex;
  align-items: center;
}
.nav-operate .updata{
  width: 40rpx;
  height: 40rpx;
  margin-left: 16rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}
.nav-operate .delete{
  width: 40rpx;
  height: 40rpx;
  margin-left: 16rpx;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}
.nav-address{
  width: 40%;
}
.nav-address .default{
  height: 28rpx;
}
.addAddress{
  margin: 42rpx 86rpx;
  height: 60rpx;
  border: 2rpx solid #ccc;
  border-radius: 10rpx;
  line-height: 56rpx;
  text-align: center;
  font-size: 10pt;
}