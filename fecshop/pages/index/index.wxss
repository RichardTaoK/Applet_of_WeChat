/* pages/index/index.wxss */
.bar{
  width: 100%;
  height: 88rpx;
  background: #f7f7f8;
  padding: 0 20rpx;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  justify-content: space-between;
  align-content: center;
}
.pull-left{
  width: 48rpx;
  height: 88rpx;
  padding: 20rpx 4rpx;
  background: url(data:image/png;base64,) no-repeat  center  center/100%  auto;
}
.pull-center{
  width: 200rpx;
  height: 88rpx;
  padding: 20rpx 4rpx;
  box-sizing: border-box;
  background: url(data:image/png;base64,) no-repeat  center  center/100%  auto;
}
.pull-right{
  width: 140rpx;
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  align-content: center;
}
.pright1{
  width: 60rpx;
  height: 88rpx;
  padding: 20rpx 10rpx;
  float: left;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}
.pright2{
  width: 60rpx;
  height: 88rpx;
  padding: 20rpx 10rpx;
  float: left;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}
swiper{
  margin-top: 88rpx;
  height: 375rpx;
}
.tjsp{
  padding: 20rpx;
  height: 340rpx;
  display: flex;
  justify-content: space-between;
}
.goods{
  padding: 20rpx;
}
.row{
  width: 100%;
  height: 724rpx;
  display: flex;
  justify-content: space-between;
  
}
.col{
  width: 340rpx;
  height: 724rpx;
}
.tp{
  width: 340rpx;
  height: 516rpx;
}
.title{
  width: 340rpx;
  height: 108rpx;
  margin: 24rpx 0;
  font-size: 24rpx;
  line-height: 36rpx;
}
.price{
  font-size: 32rpx;
}
.price:nth-child(1){
  display: inline-block;
  margin-right: 10rpx;
}
.foot_bar{
  height: 142rpx;
  color: #24292e;
  background: #F3F3F3;
}
.change_bar{
  height: 72rpx;
  padding: 16rpx;
}
.cleft{
  width: 30%;
  float: left;
  height: 36rpx;
  color: #888;
  font-size: 24rpx;
  line-height: 36rpx;
  text-align: left;
}
.cright{
  height: 40rpx;
  width: 70%;
  float: right;
  position: relative;
}
.select{
  box-sizing: border-box;
  width: 100%;
  height: 40rpx;
  border:2rpx solid #E5E5E5;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  line-height: 40rpx;
}
.select_text{
  font-size: 24rpx;
  flex: 1;
}
.select_img{
  width: 40rpx;
  height: 30rpx;
  display: block;
  transition:transform 0.3s;
}
.select_img_rotate{
  transform:rotate(180deg); 
}
.option_box{
  position: absolute;
  top: 70rpx;
  width: 100%;
  border:1px solid #efefef;
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
}
.option{
  display: block;
  line-height: 40rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #efefef;
  padding: 10rpx;
}
.foot_bottom{
  height: 150rpx;
}
.lazy{
  height: 114rpx;
  background-color: #F3F3F3;
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
}
.copy{
  height: 36rpx;
  font-size: 24rpx;
  color: #777;
  text-align: center;
}