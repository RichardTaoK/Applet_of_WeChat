/* pages/product-show/product-show.wxss */
.banner{
  height: 894rpx;
  padding: 0 40rpx;
}
.banner-img{
  height: 894rpx;
  background: no-repeat center center / 100% auto;
}
.product_info{
  padding: 16rpx 40rpx 0;
}
.product_info .nav{
  display: flex;
  justify-content: space-between;
}
.product_info .title{
  height: 40rpx;
  font-size: 11pt;
  color: #333;
}
.product_info .add-like{
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center / 100% auto;
}
.product_info .rbc_cold{
  height: 36rpx;
  margin-top: 8rpx;
  font-size: 10pt;
  display: flex;
  color: #777;
}
.product_info .item_code{
  font-size: 10pt;
  margin-top: 8rpx;
  color: #777;
}
.review_star{
  margin: 10rpx;
  display: flex;
  text-align: center;
}
.star{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center center / 100% auto;
}
.product_info .price_info{
  margin-top: 20rpx;
  height: 66rpx;
  display: flex;
  text-align: center;
}
.price_info .special_price{
  font-size: 16pt;
  color: #c00;
}
.price_info .price{
  font-size: 10pt;
  color: #333;
  margin-left: 40rpx;
  line-height: 66rpx;
  text-decoration: line-through;
}
.product_info_section{
  margin: 30rpx 0;
}
.product_info_section .pg{
  display: flex;
  margin-bottom: 20rpx;
}
.product_info_section .pg .label{
  font-size: 10pt;
}
.product_info_section .ul{
  display: flex;
  margin-left: 100rpx;
}
.product_info_section .li{
  margin-right: 20rpx;
  display: flex;
}
.product_info_section .selected{
  border: 2rpx solid red;
}
.product_info_section .img{
  width: 100rpx;
  height: 110rpx;
}
.product_info_section .size{
  width: 50rpx;
  height: 50rpx;
  font-size: 12pt;
  text-align: center;
  line-height: 50rpx;
}
.product_info_section .num{
  width: 60rpx;
  height: 36rpx;
  font-size: 8pt;
  text-align: center;
  line-height: 36rpx;
  border-top: 2rpx solid #E6E6E6;
  border-bottom: 2rpx solid #E6E6E6;
  box-sizing: border-box;
}
.product_info_section .num-but{
  width: 52rpx;
  height: 52rpx;
  text-align: center;
  font-size: 10pt;
  line-height: 40rpx;
  border: 2rpx solid #E6E6E6;
  color: red;
}
.product_description_info{
  margin-bottom: 100rpx;
}
.product_description_info .buttons-tab{
  height: 64rpx;
  display: flex;
  justify-content: space-between;
  text-align: center;
  border-bottom: 2rpx solid #E6E6E6;
}
.buttons-tab .tab-link{
  font-size: 10pt;
  color: #5f646e;
  width: 50%;
  text-align: center;
  line-height: 64rpx;
}
.buttons-tab .tab-link.active{
  color: #35A8EF;
  border-bottom: 6rpx solid #35A8EF;
}
.content-block .tab1{
  padding: 6rpx 50rpx;
  display: block;
}
.content-block .tab1 .ul{
  padding-top: 20rpx;
}
.content-block .tab1 .ul .li{
  height: 98rpx;
  border-bottom: 2rpx solid #EAEAEA;
  display: flex;
  line-height: 98rpx;
}
.content-block .tab1 .ul .li .con{
  padding-left: 20rpx;
  font-size: 10pt; 
  color: #75757A;
}
.content-block .tab1 .result-box{
  height: 468rpx;
  font-size: 8pt;
  color: #333;
  margin: 40rpx 0;
}
.content-block .img-section{
  height: 840rpx;
  margin-bottom: 10rpx;
  background: no-repeat center center / 100% auto;
}
.content-block .tab2{
  display: none;
  padding: 0 40rpx;
}
.content-block .tab2 .averageWarp{
  height: 38rpx;
  display: flex;
  text-align: center;
  margin: 40rpx 0;
  font-size: 10pt;
}
.content-block .tab2 .lbBox{
  height: 386rpx;
  margin: 30rpx 0;
  padding-bottom: 40rpx;
  border-bottom: 2rpx solid #e2e2e2;
}
.content-block .tab2 .buttons-row{
  height: 54rpx;
  padding: 0 24rpx;
  font-size: 10pt;
  display: flex;
  margin-top: 40rpx;
  color: #0894ec;
}
.content-block .tab2 .buttons-row-left{
  width: 292rpx;
  border: 2rpx solid #ccc;
  border-radius: 20rpx 0 0 20rpx;
  text-align: center;
  line-height: 54rpx;
}
.content-block .tab2 .buttons-row-right{
  width: 292rpx;
  border: 2rpx solid #ccc;
  border-radius: 0 20rpx 20rpx 0;
  text-align: center;
  line-height: 54rpx;
}
.product-Reviews_top .card{
  margin: 20rpx;
  border: 2rpx solid #E6E6E6;
  font-size: 10pt;
  color: #777;
}
.fec-card-header{
  border-bottom: 4rpx solid #E6E6E6;
  padding:  16rpx 24rpx;
  font-weight: bold;
}
.fec-card-content{
  border-bottom: 4rpx solid #E6E6E6;
  padding:  16rpx 24rpx;
}
.fec-card-content .review_name{
  margin: 20rpx 0;
}
.fec-card-footer{
  padding:  16rpx 24rpx;
  display: flex;
}
.foottap{
  width: 100%;
  height: 100rpx;
  border-top: 2rpx solid #e2e2e2;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-around;
  background: #fff;
  z-index: 10;
}
.foottap .shop{
  width: 33.3%;
  text-align: center;
  font-size: 10pt;
  position: relative;
}
.foottap .shop .shop-img{
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
  background: url(data:image/png;base64,) no-repeat center center / 100% auto;
}
.foottap .shop .shop-count{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  top: 0;
  right:0;
  border-radius: 50%;
  background: #FF0E11;
  font-size: 28rpx;
  line-height: 30rpx;
  text-align: center;
  color: #fff;
}
.drop{
  position: absolute;
  top: 0;
  right:0;
  color: #FF0E11;
  font-size: 10pt;
  animation: drop 0.6s ease;
}
@keyframes drop{
  0%{
    transform:translateY(-100rpx);
    opacity: 0;
  }

  100%{
    transform: translateY(0);
    opacity: 1;
  }
}
.foottap .add-shop{
  width: 33.3%;
  height: 100rpx;
  line-height: 100rpx;
  background: #FFC119;
  text-align: center;
  color:#fff;
}

.foottap .pay{
width: 33.3%;
  height: 100rpx;
  line-height: 100rpx;
  background: #FF0E11;
  text-align: center;
   color:#fff;
}