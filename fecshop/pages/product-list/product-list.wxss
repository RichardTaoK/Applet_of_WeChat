/* pages/product-list/product-list.wxss */
.ul .item{
  height: 254rpx;
  padding: 10rpx 0 4rpx;
}
.ul .pic{
  width: 240rpx;
  height: 240rpx;
  float: left;
  padding: 10rpx;
  background: no-repeat center center / 100% auto;
  background-origin: content-box;
}
.ul .content{
  width: 510rpx;
  height: 240rpx;
  float: left;
  padding: 0 20rpx 16rpx 20rpx;
}
.ul .title{
height: 76rpx;
line-height: 38rpx;
font-size: 10pt;
color: #333;
}
.symbol{
  display: inline-block;
  font-size: 8pt;
  color: #e93b3d;
}
.vlu{
  display: inline-block;
  font-size: 10pt;
  color: #e93b3d;

}
.pro_filter{
  height: 80rpx;
}
.pro_filter .pro_filter_items{
  height: 80rpx;
  padding: 16rpx 20rpx;
  display: flex;
  justify-content: space-between;
}
.pro_filter_items .item{
  font-size: 10pt;
  flex: 1;
  text-align: center;
}
.pro_filter_items .icon_tri{
  display: inline-block;
  margin-left: 10rpx;
  width: 30rpx;
  height: 30rpx;
  background: no-repeat center center / 100% auto;
}
.pro_filter_items .xia{
  background-image: url(data:image/png;base64,);
}
.pro_filter_items .shang{
  background-image: url(data:image/png;base64,);
}
.item_options{
  display: none;
  height: 100vh;
  position: relative;
  z-index: 3;
  background: rgba(0, 0, 0, 0.2);
}
.J_ping{
  height: 90rpx;
  background: #fff;
  line-height: 90rpx;
  font-size: 10pt;
  padding: 0 40rpx 0 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.J_ping.active{
  color: #e93b3d;
}
.duigou{
  width: 40rpx;
  height: 40rpx;
  background: no-repeat center center / 100% auto;
  background-image: url(data:image/png;base64,);
}
.filter-box{
  height: 100vh;
  background: rgba(0, 0, 0, 0.8);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 4;
  opacity: 0;
  display: none;
}
@keyframes show{
  0%{
    opacity: 0;
  }
  100%{
    opacity: 1;
  }
}
.filter-box.active{
  display: block;
  animation: show 0.6s ease forwards;
}
.filter-box .filter-box-inner{
  width: 640rpx;
  float: right;
  background: #F4F4F4;
  font-size: 10pt;
}
.filter-box-inner .item{
  background: #fff;
  padding: 0 40rpx;
  margin-top: 30rpx;
}
.filter-box-inner .item-title{
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 4rpx solid #F4F4F4;
}
.filter-box-inner .ul{
  border-bottom: 2rpx solid #F4F4F4;
}
.filter-box-inner .li{
  height: 90rpx;
  line-height: 90rpx;
  margin-left: 40rpx;
}